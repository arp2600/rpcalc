:thousand 1000 *;
:million 1000000 *;
:billion 1000000000 *;
:milli 1000 /;

// Move a number up or down the stack by n places
:mvdown swap drop ;
:mvup catch swap ;

:tuck dup drop swap catch ;

// Fibonacci numbers
:fibinit 0 1 ;
:fibnext dup`2 drop`2 swap dup drop swap catch`2 + 0 drop ;

